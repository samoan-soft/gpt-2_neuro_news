    "def load_file():\n",
    "    text = []\n",
    "    with open('./data.txt', mode='r', encoding='utf-8') as file:\n",
    "        for line in file:\n",
    "            text.append('<BOS>'+line.strip()+'<EOS>')\n",
    "    return text"
   ]
  },
  {
